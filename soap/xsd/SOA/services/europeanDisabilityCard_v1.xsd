				<xs:extension base="common:AbstractPrivacySyncRequestType">
					<xs:sequence>
						<xs:element name="request">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="personNumber" type="num:PersonNumberType"/>
									<xs:element name="language">
										<xs:simpleType>
											<xs:restriction base="iso:LanguageType">
												<xs:enumeration value="fr"/>
