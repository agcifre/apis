												<xs:enumeration value="de"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="forceCreation" type="xs:boolean"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
