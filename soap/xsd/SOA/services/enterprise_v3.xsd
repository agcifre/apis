	<xs:import namespace="http://soa.spw.wallonie.be/data/common/error/v5" schemaLocation="../data/common/error_v5.xsd"/>
	<!-- Messages -->
	<xs:element name="getEnterpriseBasicData" type="msg:GetEnterpriseBasicDataType"/>
	<xs:element name="getEnterpriseBasicDataResponse" type="msg:GetEnterpriseBasicDataResponseType"/>
	<xs:element name="getEnterpriseSummaryData" type="msg:GetEnterpriseSummaryDataType"/>
	<xs:element name="getEnterpriseSummaryDataResponse" type="msg:GetEnterpriseSummaryDataResponseType"/>
	<xs:element name="getEnterpriseExtendData" type="msg:GetEnterpriseExtendDataType"/>
	<xs:element name="getEnterpriseExtendDataResponse" type="msg:GetEnterpriseExtendDataResponseType"/>
	<xs:element name="searchEnterprise" type="msg:SearchEnterpriseType"/>
	<xs:element name="searchEnterpriseResponse" type="msg:SearchEnterpriseResponseType"/>
	<xs:element name="searchEnterpriseBySSIN" type="msg:SearchEnterpriseBySSINType"/>
	<xs:element name="searchEnterpriseBySSINResponse" type="msg:SearchEnterpriseBySSINResponseType"/>
