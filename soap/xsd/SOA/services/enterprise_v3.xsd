	<xs:element name="getAllEmploymentLevelQuartersResponse" type="msg:GetAllEmploymentLevelQuartersResponseType"/>
	<xs:element name="fault" type="err:ErrorOrWarningType"/>
	<!-- Message types -->
	<xs:complexType name="GetEnterpriseBasicDataType">
		<xs:complexContent>
			<xs:extension base="req:PrivacyRequestType">
				<xs:sequence>
					<xs:element name="request" type="entBus:GetEnterpriseRequestType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="GetEnterpriseBasicDataResponseType">
		<xs:complexContent>
			<xs:extension base="resp:ResponseType">
				<xs:sequence minOccurs="0">
					<xs:choice>
						<xs:element name="enterprise">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="basic:EnterpriseBasicType">
										<xs:attribute name="BCEInscriptionDate" type="xs:date"/>
										<xs:attribute name="validityDuration" type="xs:int"/>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="businessUnit" type="businessUnit:BusinessUnitType"/>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="GetEnterpriseSummaryDataType">
		<xs:complexContent>
			<xs:extension base="req:PrivacyRequestType">
				<xs:sequence>
					<xs:element name="request" type="entBus:GetEnterpriseRequestType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="GetEnterpriseSummaryDataResponseType">
		<xs:complexContent>
			<xs:extension base="resp:ResponseType">
				<xs:sequence minOccurs="0">
					<xs:choice>
						<xs:element name="enterprise">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="summary:EnterpriseSummaryType">
										<xs:attribute name="BCEInscriptionDate" type="xs:date"/>
										<xs:attribute name="validityDuration" type="xs:int"/>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="businessUnit" type="businessUnit:BusinessUnitType"/>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="GetEnterpriseExtendDataType">
		<xs:complexContent>
			<xs:extension base="req:PrivacyRequestType">
				<xs:sequence>
					<xs:element name="request" type="entBus:GetEnterpriseRequestType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="GetEnterpriseExtendDataResponseType">
		<xs:complexContent>
			<xs:extension base="resp:ResponseType">
				<xs:sequence minOccurs="0">
					<xs:choice>
						<xs:element name="enterprise">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="extend:EnterpriseExtendType">
										<xs:attribute name="BCEInscriptionDate" type="xs:date"/>
										<xs:attribute name="validityDuration" type="xs:int"/>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="businessUnit" type="businessUnit:BusinessUnitType"/>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SearchEnterpriseType">
		<xs:complexContent>
			<xs:extension base="req:PrivacyRequestType">
				<xs:sequence>
					<xs:element name="request" type="entBus:SearchEnterpriseRequestType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SearchEnterpriseResponseType">
		<xs:complexContent>
			<xs:extension base="resp:ResponseType">
				<xs:sequence minOccurs="0">
					<xs:choice maxOccurs="60">
						<xs:element name="enterprise">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="basic:EnterpriseBasicType">
										<xs:sequence>
											<xs:element name="juridicalForms" minOccurs="0">
												<xs:complexType>
													<xs:sequence>
														<xs:element name="juridicalForm" type="jurf:JuridicalFormType" maxOccurs="unbounded"/>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="businessUnit" type="businessUnit:BusinessUnitType"/>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SearchEnterpriseBySSINType">
		<xs:complexContent>
			<xs:extension base="req:PrivacyRequestType">
