							<xs:complexType>
								<xs:sequence>
									<xs:element name="notice" type="el:SearchNoticeResponseType" minOccurs="1" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
