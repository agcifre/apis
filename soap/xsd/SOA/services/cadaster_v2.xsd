	<!-- Criteria Types-->
	<xsd:complexType name="ListForAddressType">
		<xsd:sequence>
			<xsd:element name="includeExpired" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Include expired situations</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="situation0101" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Situation at 01/01 of the year requested</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="codeNISCommunity">
				<xsd:annotation>
					<xsd:documentation>code of the community</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:totalDigits value="5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="codeNSRStreet">
				<xsd:annotation>
					<xsd:documentation>code of the street(NSR)</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:totalDigits value="5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="language" type="cod:RequestLanguageBasicType"/>
			<xsd:element name="houseNumber" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Housenumber</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="11"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="maxResult">
				<xsd:annotation>
					<xsd:documentation>The user can choose the maximum amount of results that will be shown on the screen.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:totalDigits value="3"/>
						<xsd:minInclusive value="1"/>
						<xsd:maxInclusive value="999"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="active" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>only addressen results with status active: yes / no
                        true: give the active parcels
                        false: give the actieve AND passive addresses</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CommunitiesBySearchTokenType">
		<xsd:sequence>
			<xsd:element name="language" type="cod:RequestLanguageBasicType"/>
			<xsd:element name="searchToken" type="xsd:string"/>
