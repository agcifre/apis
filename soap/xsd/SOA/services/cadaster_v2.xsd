	<xsd:complexType name="StreetsBySearchTokenType">
		<xsd:sequence>
			<xsd:element name="language" type="cod:RequestLanguageBasicType"/>
			<xsd:element name="searchToken" type="xsd:string"/>
			<xsd:element name="codeNisComId">
				<xsd:annotation>
					<xsd:documentation>NisCom</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:totalDigits value="19"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ListForByParcelIdType">
		<xsd:annotation>
			<xsd:documentation>This method is used when the client only has a partial parcelId.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="language" type="cod:RequestLanguageBasicType"/>
			<xsd:element name="includeExpired" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Include expired situations</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="situation0101" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Situation at 01/01 of the year requested</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="active" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>only parcel results with status active: yes / no
                        yes: give the active parcels
                        no: give the actieve AND passive parcels starting from 2002</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="maxResult">
				<xsd:annotation>
					<xsd:documentation>The user can choose the maximum amount of results that will be shown on the screen.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="1"/>
						<xsd:maxInclusive value="999"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="cadastralDivision">
				<xsd:annotation>
					<xsd:documentation>Cadastral division</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:totalDigits value="5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="section">
				<xsd:annotation>
					<xsd:documentation>section</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="1"/>
						<xsd:pattern value="([A-Za-z])"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="primaryNumber" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>GroundNummer</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:totalDigits value="4"/>
						<xsd:minInclusive value="1"/>
						<xsd:maxInclusive value="9999"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="bisNumber" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>biNumber</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:totalDigits value="2"/>
						<xsd:minInclusive value="2"/>
						<xsd:maxInclusive value="99"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="exponentNumber" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>exponentNumber</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:totalDigits value="3"/>
						<xsd:minInclusive value="2"/>
						<xsd:maxInclusive value="999"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="exponentLetter" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>exponentLetter</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="1"/>
						<xsd:pattern value="([a-zA-Z])"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="partNumber" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>partnumber (P0000 -&gt; P9999)</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="5"/>
						<xsd:pattern value="([P,p][0-9][0-9][0-9][0-9])"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PropertyByPropertyIdType">
		<xsd:sequence>
			<xsd:element name="language" type="cod:RequestLanguageBasicType"/>
			<xsd:element name="includeExpired" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
