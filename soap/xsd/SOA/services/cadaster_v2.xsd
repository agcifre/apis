	<xsd:complexType name="PropertyByParcelIdType">
		<xsd:sequence>
			<xsd:element name="language" type="cod:RequestLanguageBasicType"/>
			<xsd:element name="history" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Show history of the propertystate or not</xsd:documentation>
