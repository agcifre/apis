	<xs:element name="getEnterpriseAnnualAccounts">
		<xs:annotation>
			<xs:documentation>Opération rendant les comptes annuels d'une entrprise pour une année définie.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="common:AbstractPublicSyncRequestType">
					<xs:sequence>
						<xs:element name="request" type="aa:GetEnterpriseAnnualAccountRequestType"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="getEnterpriseAnnualAccountsResponse">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="common:AbstractSyncResponseType">
					<xs:sequence minOccurs="0">
						<xs:element name="enterpriseAnnualAccount" type="aa:GetEnterpriseAnnualAccountResponseType"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="searchAvailableEnterpriseAnnualAccounts">
		<xs:annotation>
			<xs:documentation>Opération permettant de lister l'ensemble des comptes annuels disponibles</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
