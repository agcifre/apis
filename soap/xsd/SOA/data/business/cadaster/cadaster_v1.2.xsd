			<xs:element name="enterpriseNumber" type="ns1:CbeEntityNumberType" minOccurs="0"/>
			<xs:element name="denomination" type="den:DenominationType" minOccurs="0"/>
			<xs:element name="address" type="adr:AddressType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PropertyOwnerType">
