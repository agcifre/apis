				<xs:complexType>
					<xs:sequence>
						<xs:element name="propertyStatusCode" type="cod:DescriptiveCodeType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Code et description statut de la situation patrimoniale (active/passive)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="lastArticleInscription">
							<xs:annotation>
								<xs:documentation>Dernière inscription article</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="22"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="previousArticleInscription">
							<xs:annotation>
								<xs:documentation>Inscription article précédente</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="22"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="fiscalStatusPeriod" minOccurs="0">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="dat:OpenPeriodType">
										<xs:sequence>
											<xs:element name="dateBeginYear" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Année fiscale de début de la situation patrimoniale</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:integer">
														<xs:totalDigits value="4"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="dateEndYear" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Année fiscale de fin de la situation patrimoniale	</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:integer">
														<xs:totalDigits value="4"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="juridicalStatusPeriod" minOccurs="0">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="dat:OpenPeriodType"/>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="doubtfulRight" type="xs:boolean" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Droits réels douteux (true/false)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="marketValue" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="calculatedMarketValue" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Valeur vénale calculée</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:simpleContent>
												<xs:extension base="xs:decimal">
													<xs:attribute name="evaluationDate" type="xs:date">
														<xs:annotation>
															<xs:documentation>Date de la valeur vénale calculée</xs:documentation>
														</xs:annotation>
													</xs:attribute>
												</xs:extension>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="calculatedMinMarketValue" type="xs:decimal" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Valeur vénale minimum calculée</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="calculatedMaxMarketValue" type="xs:decimal" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Valeur vénale maximum calculée</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="beginDate" type="xs:dateTime">
			<xs:annotation>
				<xs:documentation>Timestamp version début situation patrimoniale</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="endDate" type="xs:dateTime">
			<xs:annotation>
				<xs:documentation>Timestamp version fin situation patrimoniale</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="cancelDate" type="xs:dateTime">
			<xs:annotation>
				<xs:documentation>Timestamp version annulation situation patrimoniale</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="PropertySituationType">
		<xs:sequence>
			<xs:element name="parcelIdentification" type="parcelIdH:ParcelIdentificationPropertySitHistType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>number of the stipad dossier</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="propertySituationIdf" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>SitPatrimoniale.oIdfTech</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="no-parcel" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Textfield for when there isn't a situation (typically before 1979) or for public domains and similar cases.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="detailApp" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Detail of appartement</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="propertySituationStatus" type="cod:DescriptiveCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Code et description statut de la situation patrimoniale (active/passive)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:sequence minOccurs="0">
				<xs:element name="dossiers" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="dossier" type="bcad_common:PropertySituationDossierType" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>The dossier in which the mutation happened</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:element name="expiredDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Expireddate of the situation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dateBegin" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Validity date of the situation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dateBeginYear" minOccurs="0">
				<xs:annotation>
					<xs:documentation>year of the start date</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="dateEnd" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>end date of situation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dateEndYear" minOccurs="0">
				<xs:annotation>
					<xs:documentation>year of the end date</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="flag-expired" type="xs:boolean" minOccurs="0"/>
			<xs:element name="flag-intermediate" type="xs:boolean" minOccurs="0"/>
			<xs:element name="article" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Article</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sequenceNumber" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Sequence</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PropertySituationDossierType">
		<xs:sequence>
			<xs:element name="dossierNumber" type="xs:string" minOccurs="0">
