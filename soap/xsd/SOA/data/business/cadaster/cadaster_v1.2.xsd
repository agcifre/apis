			<xs:extension base="ns2:RightType">
				<xs:sequence>
					<xs:element name="titulars" minOccurs="0">
						<xs:complexType>
							<xs:choice minOccurs="0">
								<xs:element name="community" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="person" type="bcad_common:PersonType" maxOccurs="2"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="person" type="bcad_common:PersonType" minOccurs="0"/>
								<xs:element name="enterprise" type="bcad_common:EnterpriseType" minOccurs="0"/>
								<xs:element name="anonymous" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="anonymousOwner" minOccurs="0">
												<xs:annotation>
													<xs:documentation>String of the anonymous owners and their businessrights (# en 2 rechthebbenden -BE 1/4- #)</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:maxLength value="100"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="association" minOccurs="0">
									<xs:complexType>
										<xs:sequence minOccurs="0">
											<xs:element name="person" type="bcad_common:PersonType" minOccurs="0" maxOccurs="unbounded"/>
											<xs:element name="enterprise" type="bcad_common:EnterpriseType" minOccurs="0" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:choice>
						</xs:complexType>
					</xs:element>
					<xs:element name="coOwner" minOccurs="0">
						<xs:annotation>
							<xs:documentation>String of the coOwners</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="100"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="serviceManagerId" minOccurs="0">
						<xs:annotation>
							<xs:documentation>PersIdf manager service
PersIdf service gestionnaire
PersIdf beherende dienst</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="12"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ParcelLocationType">
