				</xs:annotation>
			</xs:element>
			<xs:element name="dateValidationDossier" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Validitydate of the parcel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!--
			<xs:element name="searchedPropertySituation" type="ws:pathislSp" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="previousPropertySituation" type="ws:pathislSp" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="nextPropertySituation" type="ws:pathislSp" minOccurs="0" maxOccurs="unbounded"/>
			-->
		</xs:sequence>
	</xs:complexType>
