	<xs:complexType name="EnvironmentalLicenceSummaryType">
		<xs:complexContent>
			<xs:extension base="approv:licenceType">
				<xs:sequence>
					<xs:element name="establishments" type="tns:establishmentSummaryType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Etablissement(s) lié(s) au dossier. Pour la majeure partie des types de dossier, un seul établissement est associé. Pour certains, par exemple les recours, plusieurs établlisements pourraient être concerné par la demande de permis</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EnvironmentalLicenceType">
		<xs:complexContent>
			<xs:extension base="approv:licenceType">
				<xs:sequence>
					<xs:element name="administrativeProcess" type="tns:AdministrativeProcessType">
						<xs:annotation>
							<xs:documentation>Procédure administrative - Caractéristiques spécifiques aux permis d'environnement</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="electronicForm" type="tns:ElectronicFormType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Formulaire (Dossier) électronique - Caractéristiques spécifiques au formulaire électronique correspondant au permis d'environnement</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="establishments" type="tns:EstablishmentType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Etablissement(s) lié(s) au dossier. Pour la majeure partie des types de dossier, un seul établissement est associé. Pour certains, par exemple les recours, plusieurs établlisements pourraient être concerné par la demande de permis</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="requesters" type="tns:RequesterType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Demandeurs (Exploitants)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="solicitors" type="tns:SolicitorType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Autres demandeurs (Solliciteur, Requérants, Preneur...)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="municipalitySpecification" type="tns:municipalitySpecificationType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Caractéristique liées à la commune de dépôt de la demande de permis</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="dgo3Specification" type="tns:dgo3SpecificationType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Caractéristique liées à la DGO3</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="dgo4Specification" type="tns:dgo4SpecificationType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Caractéristique liées à la DGO4</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="documents" type="tns:documentType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Documents de la demande</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="establishmentCaracteristics">
