			<xs:element name="replacingPersonNumber" type="num:PersonNumberType" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Update"/>
					<xs:enumeration value="Replacement"/>
					<xs:enumeration value="Expiration"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="householdMutation" type="mutev:HouseholdMutationType"/>
	<xs:complexType name="ObservedMutationType">
		<xs:sequence>
			<xs:element name="personNumber" type="num:PersonNumberType"/>
			<xs:element name="modificationDate"/>
		</xs:sequence>
	</xs:complexType>
