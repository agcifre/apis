<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
		xmlns:pro="http://soa.spw.wallonie.be/data/entity/person/profession/v1" 
		xmlns:cod="http://soa.spw.wallonie.be/data/common/code/v1" 
		xmlns:lab="http://soa.spw.wallonie.be/data/common/label/v1" 
		targetNamespace="http://soa.spw.wallonie.be/data/entity/person/profession/v1" 
		elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.00">

	<xs:annotation>
		<xs:documentation>
			Definitions of profession-related types.
		</xs:documentation>
	</xs:annotation>

	<xs:import namespace="http://soa.spw.wallonie.be/data/common/code/v1" schemaLocation="../../common/code_v1.xsd"/>
	<xs:import namespace="http://soa.spw.wallonie.be/data/common/label/v1" schemaLocation="../../common/labels_v1.xsd"/>

	<xs:complexType name="ProfessionType">
		<xs:annotation>
			<xs:documentation>
				The name of a person consists of zero or more first names, and one or more last names.
				There is an optional indicator stating which of the first names is the used one. (Some
				people use their second - or other - name as a first name.)
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="occupation" type="cod:CodeType" minOccurs="0" nillable="true"/>
			<xs:element name="socialcategory" type="cod:CodeType" minOccurs="0" nillable="true"/>
			<xs:element name="comment1" type="lab:CommentType" minOccurs="0" nillable="true"/>
			<xs:element name="comment2" type="lab:CommentType" minOccurs="0" nillable="true"/>
		</xs:sequence>
	</xs:complexType>


</xs:schema>
