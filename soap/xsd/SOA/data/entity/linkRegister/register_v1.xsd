<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://soa.spw.wallonie.be/data/entity/linkRegister/register/v1" xmlns:link="http://soa.spw.wallonie.be/data/entity/linkRegister/link/v1" xmlns:num="http://soa.spw.wallonie.be/data/entity/person/number/v2" targetNamespace="http://soa.spw.wallonie.be/data/entity/linkRegister/register/v1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.00">
	<xsd:import namespace="http://soa.spw.wallonie.be/data/entity/person/number/v2" schemaLocation="../person/personNumber_v2.xsd"/>
	<xsd:import namespace="http://soa.spw.wallonie.be/data/entity/linkRegister/link/v1" schemaLocation="link_v1.xsd"/>
	<xsd:simpleType name="ForeignIdType">
		<xsd:restriction base="xsd:string">
