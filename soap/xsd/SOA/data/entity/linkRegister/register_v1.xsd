			<xsd:maxLength value="64"/>
			<xsd:whiteSpace value="collapse"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ForeignIdTypeType">
		<xsd:restriction base="xsd:string">
