	</xsd:simpleType>
	<xsd:simpleType name="DescriptionType">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="100"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="PersonNumberWithCanceledAndReplacedType">
		<xsd:simpleContent>
			<xsd:extension base="num:PersonNumberType">
