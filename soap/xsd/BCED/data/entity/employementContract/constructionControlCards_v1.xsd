<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:construct="http://bced.wallonie.be/data/entity/employementContract/constructionControlCard/v1" targetNamespace="http://bced.wallonie.be/data/entity/employementContract/constructionControlCard/v1" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="1.0">
	<xs:complexType name="ConstructionControlCardType">
		<xs:annotation>
			<xs:documentation>Carte de contrôle C3.2A (donnée spécifique pour la construction, commission paritaire 124)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="firstMonthC32ANumber" type="construct:C32ANumberType" minOccurs="0"/>
			<xs:element name="nextMonthC32ANumber" type="construct:C32ANumberType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="C32ANumberType">
		<xs:restriction base="xs:long">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="999999999999"/>
			<xs:totalDigits value="12"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
