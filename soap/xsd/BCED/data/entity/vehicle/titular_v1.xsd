<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://bced.wallonie.be/data/entity/vehicle/titular/v1" xmlns:num="http://bced.wallonie.be/data/entity/person/number/v1" xmlns:ns1="http://bced.wallonie.be/data/entity/enterprise/enterprise/v2" targetNamespace="http://bced.wallonie.be/data/entity/vehicle/titular/v1" elementFormDefault="qualified">
	<xs:import namespace="http://bced.wallonie.be/data/entity/enterprise/enterprise/v2" schemaLocation="../enterprise/enterprise_v2.xsd"/>
	<xs:import namespace="http://bced.wallonie.be/data/entity/person/number/v1" schemaLocation="../person/personNumber_v1.xsd"/>
	<!-- Person ou CBE -->
	<xs:complexType name="TitularType">
		<xs:sequence>
			<xs:element name="titularId" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:choice>
				<xs:sequence>
					<xs:element name="personNumber" type="num:PersonNumberType"/>
				</xs:sequence>
				<xs:sequence>
					<xs:element name="enterpriseNumber" type="ns1:CbeEntityNumber"/>
				</xs:sequence>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
