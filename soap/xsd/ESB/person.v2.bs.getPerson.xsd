			<xs:element name="civilState" type="xs:boolean"/>
			<xs:element name="administrator" type="xs:boolean"/>
			<xs:element name="subregister" type="xs:boolean"/>
			<xs:element name="legalCohabitation" type="xs:boolean"/>
			<xs:element name="photo" type="xs:boolean"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
