												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="registrationBailiff" minOccurs="0">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="bailiffName" type="xs:string"/>
												<xs:element name="location" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Place where civil state was registered.</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="countryCode" type="xs:string"/>
															<xs:element name="countryName" minOccurs="0" maxOccurs="3">
																<xs:complexType>
																	<xs:simpleContent>
																		<xs:extension base="xs:string">
																			<xs:attribute name="language" type="xs:string" use="optional"/>
																		</xs:extension>
																	</xs:simpleContent>
																</xs:complexType>
															</xs:element>
															<xs:element name="cityCode" type="xs:string" minOccurs="0"/>
															<xs:element name="cityName" minOccurs="0" maxOccurs="3">
																<xs:complexType>
																	<xs:simpleContent>
																		<xs:extension base="xs:string">
																			<xs:attribute name="language" type="xs:string" use="optional"/>
																		</xs:extension>
																	</xs:simpleContent>
																</xs:complexType>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="inceptionDate" type="xs:date" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="photo" minOccurs="0">
				<xs:complexType>
					<xs:choice>
						<xs:element name="unauthorizedPhoto" type="xs:boolean"/>
						<xs:element name="photoMissing" type="xs:boolean"/>
						<xs:element name="jpeg">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="captureDate" type="xs:date" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Capture date of the photo.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="encodedPhoto" type="xs:base64Binary">
										<xs:annotation>
											<xs:documentation>Base64 encoded photo.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="register" type="xs:string"/>
		<xs:attribute name="state">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="ALIVE">
						<xs:annotation>
							<xs:documentation>
						A person who is ALIVE should have birth-related information.
					</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="DECEASED">
						<xs:annotation>
							<xs:documentation>
						A person who is deceased should have both birth-related and decease-related infotmation.
					</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="DISAPPEARED">
						<xs:annotation>
							<xs:documentation>
						A person who is disappeared may be lacking certain information which is otherwise
						present for persons in any of the other states. This is a special case of the unknown
						state.
					</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="UNKNOWN">
						<xs:annotation>
							<xs:documentation>
						A person is in an unknown state if it is unkown whether he is deceased or not, and
						he is not disappeared. Certian information related to birth and decease may be unknown.
					</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="source">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="RN/RR"/>
					<xs:enumeration value="BCSS/KSZ">
						<xs:annotation>
							<xs:documentation>
						A code defined by the
						Banque Carrefour de la Securite Sociale (fr) / Kruispuntbank der Sociale Zekerheid (nl)
					</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="searchPersonByName">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="common:AbstractRequestType">
					<xs:sequence>
						<xs:element name="filter" type="FilterType"/>
						<xs:element name="criteria">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="firstName" type="xs:string" maxOccurs="3"/>
									<xs:element name="lastName" type="xs:string"/>
									<xs:element name="birthDate" type="xs:date"/>
									<xs:element name="bithDateTolerence" type="xs:nonNegativeInteger"/>
									<xs:element name="gender" minOccurs="0">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="M"/>
												<xs:enumeration value="F"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="nisCode" type="xs:string" minOccurs="0"/>
									<xs:element name="register" type="xs:string" minOccurs="0"/>
									<xs:element name="maxReturnData" minOccurs="0">
										<xs:simpleType>
											<xs:restriction base="xs:positiveInteger">
												<xs:maxInclusive value="50"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="searchPersonByNameResponse">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="common:AbstractResponseType">
					<xs:sequence>
						<xs:element name="person" type="PersonType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="searchPersonByAddress">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="common:AbstractRequestType">
					<xs:sequence>
						<xs:element name="filter" type="FilterType"/>
						<xs:element name="criteria">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="postalCode" type="xs:string"/>
									<xs:element name="streetCode" type="xs:string"/>
									<xs:element name="houseNumber" type="xs:string"/>
									<xs:element name="houseIndex" type="xs:string" minOccurs="0"/>
									<xs:element name="maxReturnData" minOccurs="0">
										<xs:simpleType>
											<xs:restriction base="xs:positiveInteger">
												<xs:maxInclusive value="50"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="typeOfPersons" minOccurs="0">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="ALL"/>
												<xs:enumeration value="HEAD_OF_FAMILY"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="page" type="xs:nonNegativeInteger" minOccurs="0">
										<xs:annotation>
											<xs:documentation>
											The page number of the search results. Defaults to one.
										</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="searchPersonByAddressResponse">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="common:AbstractResponseType">
					<xs:sequence>
						<xs:element name="person" type="PersonType" maxOccurs="unbounded"/>
